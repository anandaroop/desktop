{
  "name": "@anandaroop/desktop",
  "description": "Easily show/hide the icons on your desktop. Great for screenshots and screencaps.",
  "version": "0.0.5",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "Anandaroop Roy @anandaroop",
  "bin": {
    "desktop": "./bin/run"
  },
  "bugs": "https://github.com/anandaroop/desktop/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.2",
    "shelljs": "^0.8.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/chalk": "^2.2.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/shelljs": "^0.8.5",
    "chai": "^4",
    "globby": "^10",
    "mocha": "^5",
    "np": "^5.0.3",
    "nyc": "^13",
    "oclif": "^1.13.5",
    "prettier": "^1.18.2",
    "sinon": "^7.4.1",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/anandaroop/desktop",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "desktop",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "anandaroop/desktop",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "release": "np"
  },
  "types": "lib/index.d.ts"
}
